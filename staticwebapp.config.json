{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  },
  "customDomains": {
    "domains": [
      {
        "domain": "elitewaylimo.ch",
        "ssl": true
      },
      {
        "domain": "www.elitewaylimo.ch",
        "ssl": true
      }
    ]
  }
}