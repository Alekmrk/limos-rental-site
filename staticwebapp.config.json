{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  },
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/index.html"
    }
  ]
}