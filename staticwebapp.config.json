{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "methods": ["GET"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{png,jpg,gif,svg,ico,woff,ttf,eot}"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "Vary": "*",
    "Access-Control-Allow-Origin": "*",
    "X-Deploy-Time": "${VITE_BUILD_TIME}",
    "X-Version": "v2-${VITE_DEPLOY_ID}"
  },
  "mimeTypes": {
    ".json": "text/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".map": "text/plain"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}